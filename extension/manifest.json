{
  "name": "Full Page Screen Capture",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Screen capture your current page",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab" ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
